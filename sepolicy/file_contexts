
# Camera
/data/cam_socket.*                          u:object_r:camera_socket:s0
/dev/gemini.*                               u:object_r:camera_device:s0

# Keys
/sys/devices/platform/gpio-keys/disabled_keys   u:object_r:sysfs_keypad:s0

# GPS
/data/misc/gpsone_d(/.*)?                   u:object_r:location_data_file:s0
/data/misc/gsiff_ctrl_q                     u:object_r:location_data_file:s0
/data/misc/quipc                            u:object_r:location_data_file:s0

# Lights
/dev/SN3193                                 u:object_r:lights_device:s0

# Oppo NV
/data/opponvitems(/.*)?                     u:object_r:opponvitem_file:s0

# We have a couple of non-standard NV partitions
/dev/block/platform/msm_sdcc.1/by-name/oppodycnvbk u:object_r:modem_efs_partition_device:s0
/dev/block/platform/msm_sdcc.1/by-name/oppostanvbk u:object_r:modem_efs_partition_device:s0

/persist/sns.reg    u:object_r:sensors_persist_file:s0

/dev/block/mmcblk0p1.* u:object_r:modem_block_device:s0
/dev/block/mmcblk0p2.* u:object_r:modem_block_device:s0

